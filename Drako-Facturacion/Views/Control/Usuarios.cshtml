@model List<Drako_Facturacion.Models.ViewModel.ListTablaViewModel>
@{
    ViewBag.Title = "Usuarios";
}

<h2>@ViewBag.Title</h2>

<div class="row">
    <div class="col-lg-6">
        <table class="table ">
            <tr>
                <th>#</th>
                <th>Email</th>
                <th>Password</th>
                <th>IdState</th>
            </tr>
            @foreach (var oElemento in Model)
            {
                <tr>
                    <td>@oElemento.Id</td>
                    <td>@oElemento.Email</td>
                    <td>@oElemento.Passwords</td>
                    <td>@oElemento.IdState</td>
                </tr>
            }
        </table>
    </div>
</div>

